SELECT moveCol,scoreCol
FROM #tableName#
WHERE (
	'#whitePawns#' LIKE (
		SELECT whitePawnCol
		FROM (
			SELECT ROWID
			FROM #tableName#
		)
		WHERE (
			'#blackPawns#' LIKE (
				SELECT blackPawnCol
				FROM (
					SELECT ROWID
					FROM #tableName#
				)
				WHERE (
					'#whiteKings#' LIKE (
						SELECT whiteKingCol
						FROM (
							SELECT ROWID
							FROM #tableName#
						)
						WHERE (
							'#blackKings#' LIKE (
								SELECT blackKingCol
								FROM (
									SELECT ROWID
									FROM #tableName#
								)
							)
						) >= #blackKingThreshold#
					)
				) >= #whiteKingThreshold#
			)
		) >= #blackPawnThreshold#
	)
) >= #whitePawnThreshold#
ORDER BY scoreCol DESC