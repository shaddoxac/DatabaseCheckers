SELECT MoveDescription, Score FROM #tableName#
WHERE '#whitePawns#' LIKE (
		SELECT WhitePawns FROM #tableName#
		WHERE '#blackPawns#' LIKE (
				SELECT BlackPawns FROM #tableName#
				WHERE '#whiteKings#' LIKE (
						SELECT WhiteKings FROM #tableName#
						WHERE '#blackKings#' LIKE (
								SELECT BlackKings FROM #tableName#
						)
				)
		)
)
ORDER BY score DESC;